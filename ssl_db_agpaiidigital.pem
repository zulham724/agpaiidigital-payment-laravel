<div id="login_area" class="container">
    <div id="login_box">
        <div id="brand_area" class="text-center">
            <a href="/" class="brand-header"><img src="/public/images/logo-full.png" alt="ScaleGrid" /></a>
            <p class="text-muted text-center">
                Use your email address and password to login.<br />
                Don't have an account yet? <a href="/users/register">Sign Up</a> now.
            </p>
        </div>
        <div id="login_form_box">
            <img class="img-loader" src="/public/images/loading.gif" />
            <div id="errors" class="alert alert-danger hide"></div>
            <form id="login_form" role="form" novalidate>
                <div id="main_login_area">
                    <div class="form-group">
                        <label for="username">
                            <i class="fa fa-envelope form-icon"></i>
                            Email Address
                        </label>
                        <input type="email" id="username" name="username" class="form-control" placeholder="Email" autocomplete="off">
                    </div>
                    <div class="form-group">
                        <label for="password">
                            <i class="fa fa-unlock-alt form-icon"></i>
                            Password
                        </label>
                        <span class="password-group">
                            <input type="password" id="password" name="password" class="form-control" placeholder="Password">
                            <div class="caps-area"><img src="/public/images/caps.png" class="caps-warning" /></div>
                        </span>
                    </div>
                </div>
                <div class="form-group">
                    <input type="submit" id="btn_login" name="user_submit" class="btn btn-primary btn-block" title="Login" value="Login" disabled />
                </div>
                <div class="form-group text-center">
                    <a href="/application/beginpasswordreset" class="btn btn-link forgot-pass">Forgot Password?</a>
                </div>
            </form>
            <form id="two_factor_form" class="hide" role="form" novalidate>
                <div class="form-group">
                    <label for="verify_code">
                        <i class="fa fa-key form-icon"></i>
                        Enter two factor authentication code
                    </label>
                    <input type="text" id="verify_code" name="inputCode" class="form-control" autocomplete="off">
                </div>
                <div class="form-group">
                    <input type="submit" id="btn_verify" name="verify_submit" class="btn btn-primary btn-block" title="Login" value="Login" />
                    <a href="/" id="return_to_login" class="btn btn-link center-block">
                        <i class="fa fa-long-arrow-left form-icon"></i>
                        Return to Login
                    </a>
                </div>
            </form>
        </div>
    </div>
    <footer>
        <p class="copyright">&copy; ScaleGrid</p>
    </footer>
</div>
